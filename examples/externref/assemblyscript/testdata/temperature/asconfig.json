{
  "targets": {
    "debug": {
      "outFile": "build/debug.wasm",
      "textFile": "build/debug.wat",
      "sourceMap": true,
      "debug": true
    },
    "release": {
      "outFile": "../temperature.wasm",
      "textFile": "build/release.wat",
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "converge": false,
      "noAssert": false,
      "bindings": "none"
    }
  },
  "options": {
    "bindings": "esm",
    "enable": "reference-types"
  }
}
